{
  "name": "kanban-task-management-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "node server/server.cjs",
    "dev:server": "concurrently \"npm run dev\" \"npm run server\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier:check": "npx prettier --check **/*.{ts,tsx}",
    "prettier:write": "npx prettier --write **/*.{ts,tsx}",
    "steiger": "npx steiger ./src",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-redux": "^7.1.25",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "sass": "^1.69.5"
  },
  "devDependencies": {
    "@types/json-server": "^0.14.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "concurrently": "^9.0.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "json-server": "^0.17.4",
    "lodash": "^4.17.21",
    "lodash.merge": "^4.6.2",
    "prettier": "^3.3.3",
    "steiger": "^0.4.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-eslint": "^1.8.1"
  }
}
